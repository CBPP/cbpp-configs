	<icon>catfish</icon>
	<name>Find files</name>
	<unique-id>1557619282803842-1</unique-id>
	<command>catfish --hidden --path=%f</command>
	<description>Search for files with Catfish</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>terminator</icon>
	<name>Open terminal here</name>
	<unique-id>1557619282803851-2</unique-id>
	<command>terminator --working-directory %f</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>terminator</icon>
